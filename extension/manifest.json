{
    "manifest_version": 2,
    "name": "GPII chrome extension",
    "version": "0.1.0",
    "permissions": [
        "management",
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "content_scripts": [{
        "matches": [
            "http://*/",
            "https://*/",
            "<all_urls>"
        ],
        "css": ["css/highContrast.css"]
    }],
    "background": {
        "scripts": [
            "dist/gpii-chrome-extension-all.min.js",
            "src/background.js"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
